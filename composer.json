{
    "name": "endroid/gcm-bundle",
    "description": "Endroid Google Cloud Messaging Bundle for Symfony",
    "keywords": ["endroid", "gcm", "push", "notification", "notifications", "google", "cloud", "messaging", "symfony", "bundle"],
    "homepage": "https://github.com/endroid/EndroidGcmBundle",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "http://endroid.nl/"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "endroid/gcm": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Bundle\\GcmBundle\\": [ "src/", "tests/" ]
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
